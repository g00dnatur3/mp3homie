{
  "name": "mp3homie",
  "version": "0.0.1",
  "description": "mp3 homie",
  "scripts": {
    "lint": "tslint 'src/**/*.ts?(x)'",
    "lint-fix": "tslint --fix 'src/**/*.ts?(x)'",
    "start": "concurrently --kill-others \"npm run start-express\"",
    "start-express": "NODE_ENV=production node build/server/express/index.js",
    "build-express": "rm -rf build && node_modules/.bin/tsc && npm run copy-all && webpack --mode production",
    "copy-all": "npm run copy-ssl",
    "copy-ssl": "cp -a src/server/ssl build/server/",
    "build": "npm run build-express",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --host 0.0.0.0",
    "server": "nodemon build/server/express/index.js",
    "dev": "rm -rf build && node_modules/.bin/tsc && npm run copy-all && concurrently --kill-others \"node_modules/.bin/tsc -w\" \"npm run server\" \"npm run client\""
  },
  "author": "unknown",
  "license": "ISC",
  "dependencies": {
    "@distube/ytdl-core": "^4.14.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.5.0",
    "core-js": "^3.14.0",
    "executive": "^1.6.3",
    "express": "^4.16.4",
    "fluent-ffmpeg": "^2.1.2",
    "format-duration": "^1.3.1",
    "log4js": "^3.0.6",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.7",
    "npm": "^6.14.8",
    "query-string": "^6.13.7",
    "react": "^17.0.2",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "readline": "^1.3.0",
    "sanitize-filename": "^1.6.3",
    "scrape-youtube": "^2.4.0",
    "serve-favicon": "^2.5.0",
    "smoothscroll-polyfill": "^0.4.4",
    "undici": "^5.22.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.23",
    "@types/morgan": "^1.7.35",
    "@types/node": "^18.7.14",
    "@types/query-string": "^6.2.0",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^3.6.1",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "nodemon": "^1.18.9",
    "react-svg-loader": "^2.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "supertest": "^4.0.2",
    "ts-loader": "6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
